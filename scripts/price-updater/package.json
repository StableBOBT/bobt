{
  "name": "bobt-price-updater",
  "version": "1.0.0",
  "description": "Updates BOBT Oracle with P2P prices from CriptoYa",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "update-prices": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stellar",
    "soroban",
    "oracle",
    "bobt",
    "stablecoin",
    "bolivia"
  ],
  "author": "BOBT Team",
  "license": "MIT",
  "dependencies": {
    "@stellar/stellar-sdk": "^12.3.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
